{
  "name": "vue-fn",
  "version": "0.4.1-rc",
  "description": "A Vue 3 reactive function utility library providing domain-driven design patterns and reactive state management utilities.",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "private": true,
  "keywords": [
    "vue",
    "vue3",
    "vue-utils",
    "reactivity",
    "reactive",
    "state",
    "state-management",
    "shared-state",
    "cross-tab",
    "domain-driven-design",
    "event-driven",
    "aggregation",
    "event-bus",
    "plugin-system",
    "broadcast-channel",
    "typescript",
    "composition-api",
    "server-side",
    "ssr"
  ],
  "author": {
    "name": "AlphaFoxz",
    "url": "https://github.com/AlphaFoxz"
  },
  "license": "UNLICENSED",
  "homepage": "https://alphafoxz.github.io/vue-fn/zh/",
  "repository": "https://github.com/AlphaFoxz/vue-fn",
  "scripts": {
    "build": "run-p build:pack && run-p verify && run-p test && run-p build:sync",
    "build:pack": "vite build && node scripts/gen-package-info.mjs",
    "build:sync": "ncp ./README.md ./dist/README.md",
    "verify": "tsc -p tsconfig.build.json --composite false",
    "test": "vitest --coverage --run",
    "test-ui": "vitest --coverage --ui"
  },
  "dependencies": {
    "immer": "^10.2.0",
    "ts-deferred": "^1.0.4"
  },
  "peerDependencies": {
    "@vue/reactivity": ">=3.5.0",
    "vue": ">=3.5.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "@vue/reactivity": {
      "optional": true
    }
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.5",
    "@types/node": "^22.19.3",
    "@vitest/coverage-v8": "4.0.16",
    "@vitest/ui": "4.0.16",
    "@vue/reactivity": "^3.5.26",
    "ncp": "^2.0.0",
    "npm-run-all2": "^8.0.4",
    "read-pkg": "^10.0.0",
    "type-fest": "^5.3.1",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vitest": "^4.0.16",
    "vue": "^3.5.26"
  }
}
