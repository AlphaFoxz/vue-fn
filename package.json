{
  "name": "vue-fn",
  "version": "0.0.3-aplha.2",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "private": true,
  "scripts": {
    "build": "pnpm build:pack && pnpm build:check && pnpm test && pnpm build:sync",
    "build:pack": "vite build && node scripts/gen-package-info.mjs",
    "build:check": "tsc -p tsconfig.build.json --composite false",
    "build:sync": "ncp ./package.publish.json ./dist/package.json",
    "test": "npx jest"
  },
  "dependencies": {
    "@vue/reactivity": "^3.5.12"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.5",
    "jest": "^29.7.0",
    "ncp": "^2.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.9"
  }
}
